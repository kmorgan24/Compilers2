program
{
    int a;
    int b;
    int c;
    a = 1;
    b = 2;
    
    while(a == 1 || b == 1)    //prints 2 once
        a = a + 1;
    print(a);       
    a = 1;
    while(a == 2 || b == 2)    //prints 3 once
        b = b + 1;
    print(b);
    b = 2;
    while(a == 2 || b == 1)    // both wrong prints nothing
        print(5);
    
    while(a == 1 && b == 2)    // both true, prints 3 one time
        b = b + 1;
    print(b);
    b = 2;
    while(a == 2 && b == 2)    // first wrong prints nothing
        print(6);
    
    while(a == 1 && b == 1)    //second wrong prints nothing
        print(7);
    
    while(a == 2 && b == 1)    // both wrong prints nothing
        print(8);
    
    while(a != 5)
        a = a + 1;       // prints 5
    print(a);
    print(9);
}